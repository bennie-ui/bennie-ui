{
  "name": "benie-ui",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "build": "panda cssgen --outfile=.storybook/panda-generated.css && storybook build",
    "codegen": "panda codegen --watch",
    "deploy": "wrangler pages deploy ./storybook-static",
    "start": "wrangler pages dev ./storybook-static",
    "dev": "panda cssgen --outfile=.storybook/panda-generated.css && storybook dev -p 6006"
  },
  "workspaces": [
    "libraries/*",
    "components/primitives/*",
    "components/composites/*",
    "components/composites/forms/*",
    "components/composites/feedback/*"
  ],
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@loadable/component": "^5.16.4",
    "@remix-run/react": "^2.8.1",
    "@vitejs/plugin-react": "^4.3.4",
    "install": "^0.13.0",
    "react": "^18.2.0",
    "tsconfck": "^3.1.6"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^13.6.2",
    "@pandacss/dev": "^1.8.0",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-onboarding": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/react-vite": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "autoprefixer": "^10.4.19",
    "bun-types": "^1.0.16",
    "postcss": "^8.4.3",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.1.0",
    "prop-types": "^15.8.1",
    "react-dom": "^18.2.0",
    "storybook": "^8.0.4",
    "tailwindcss": "^3.4.1",
    "wrangler": "^3.92.0"
  }
}
